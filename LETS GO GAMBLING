		  	num[i] = valor
		}
		
	     escreva("agr digite um numero: ")
	     leia(gambling)
	      
	     para(inteiro i=0; i < 30; i++){
	      	
	      	se(gambling == num[i]){
	      		escreva("acertou!, a posiÃ§ao do numero e:", i, "\n")
	      	}
	      }

	}

		 

		
		
}
