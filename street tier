            buxas[5] = "DEKU"
           
		 para(inteiro i=0; i < 5; i++){
             escreva(i + 1, "lugar: º", buxas[i], "\n")
	}
           para(inteiro i=0; i < 6; i++){
            se(buxas[i] == "IZUKU"){
            	escreva("VEM CÁ STREET TIER")
